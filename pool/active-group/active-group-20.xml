    <history>
        <item date="2025-04-17">imported from Active Group sample exam</item>
    </history>

    <refersToLgs>
        <lg curriculumVersion="2025.1" lg="02-02" />
    </refersToLgs>

    <stem>
        <text xml:lang="de">
            Welche Einflüsse müssen Softwarearchitekt:innen erkennen und berücksichtigen?
        </text>
        <text xml:lang="en">
            What influences does a software architect have to recognize and take into account?
        </text>
    </stem>

    <categoryStatements>

        <categories>
            <category label="a">
                <text xml:lang="de">trifft zu</text>
                <text xml:lang="en">True</text>
            </category>
            <category label="b">
                <text xml:lang="de">trifft nicht zu</text>
                <text xml:lang="en">False</text>
            </category>
        </categories>

        <statements>
            <statement correctCategory="a" identifier="A">
                <text xml:lang="de">
                    Einfluss von Qualitätsanforderungen
                </text>
                <text xml:lang="en">
                    Influence of quality requirements
                </text>
            </statement>
            <statement correctCategory="b" identifier="B">
                <text xml:lang="de">
                    Eigene Vorlieben für bestimmte Komponenten
                </text>
                <text xml:lang="en">
                    Customer preferences for certain components
                </text>
            </statement>
            <statement correctCategory="a" identifier="C">
                <text xml:lang="de">
                    Gesetzliche Faktoren
                </text>
                <text xml:lang="en">
                    Legal factors
                </text>
            </statement>
            <statement correctCategory="a" identifier="D">
                <text xml:lang="de">
                    Einfluss verschiedener Stakeholder
                </text>
                <text xml:lang="en">
                    Influence of different stakeholders
                </text>
            </statement>
            <statement correctCategory="a" identifier="E">
                <text xml:lang="de">
                    Technische Vorgaben
                </text>
                <text xml:lang="en">
                    Technical guidelines
                </text>
            </statement>
        </statements>
    </categoryStatements>
</categoryQuestion>
